<?xml version="1.0"?>
<robot name="visual">

<material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="frame">
    <visual>
      <geometry>
        <box size="1 0.5 0.5"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <link name="right_front_wheel">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="right_front_wheel_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
    <parent link="frame"/>
    <child link="right_front_wheel"/>
    <origin rpy="1.57 0 0" xyz="0.4 -0.25 -0.2"/>
  </joint>

  <link name="right_back_wheel">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="right_back_wheel_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
    <parent link="frame"/>
    <child link="right_back_wheel"/>
    <origin rpy="1.57 0 0" xyz="-0.4 -0.25 -0.2"/>
  </joint>

  <link name="left_front_wheel">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="left_front_wheel_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
    <parent link="frame"/>
    <child link="left_front_wheel"/>
    <origin rpy="1.57 0 0" xyz="0.4 0.35 -0.2"/>
  </joint>

  <link name="left_back_wheel">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <cylinder length="0.1" radius="0.15"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="left_back_wheel_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
    <parent link="frame"/>
    <child link="left_back_wheel"/>
    <origin rpy="1.57 0 0" xyz="-0.4 0.35 -0.2"/>
  </joint>


</robot>
